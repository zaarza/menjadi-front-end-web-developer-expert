{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": "airbnb-base",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
      "no-underscore-dangle": 0,
      "no-console": 0,
      "no-useless-return": 0,
      "import/no-extraneous-dependencies": 0,
      "import/no-import-module-exports": 0
    }
}
